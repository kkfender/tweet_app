<div class="follow-container">
 

  <% @users.all_following.each do |f|%>
 <div class="user-profile">
   
   <div class="user-list-image">
    <img src="<%= "/user_images/#{f.image_name}" %>">
 </div>
 <%=link_to(f.name,"/users/#{f.id}",class:"show-index")%>
    <%if @current_user.followed_by? (f)%>
       <div class="back-message">フォローされています</div>
     <%end%>
 </div>
    <%end%>
    
</div>