<div class="user-list">
  <%= will_paginate %>
    <% @users.each do |user| %>
      <div class="user-profile">
        <div class="user-list-image">
          <%= image_tag user.img.url,:class => "user-image" %>
        </div>
          <div class="prof">
            <%=link_to(user.name,"/users/#{user.id}",class:"show-index")%>
          </div>
            <div class="user-profile-page">
              <% user.tag_list.each do |tag| %>
  <span class="label label-primary"><%= tag %></span>
  


<% end %>
           </div> 
      </div>
 

   
<% end %>
  <%= will_paginate %>
</div>
 
 
